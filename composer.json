{
    "name": "coderius/yii2-pell-widget",
    "description": "Pell content editor widget for Yii2.",
    "type": "yii2-extension",
    "keywords": [
        "coderius",
        "yii",
        "yii2",
        "yii 2",
        "extension",
        "widget",
        "pell",
        "editor"
    ],
    "homepage": "https://github.com/jaredreich/pell",
    "license": "MIT",
    "authors": [
        {
            "name": "Serg Coderius",
            "email": "sunrise4fun@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/coderius/yii2-pell-widget/issues",
        "source": "https://github.com/coderius/yii2-pell-widget"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "yiisoft/yii2": "^2.0",
        "npm-asset/pell": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "coderius\\pell\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
